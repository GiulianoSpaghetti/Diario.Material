<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             xmlns:pages="clr-namespace:Diario.Pages"
			 x:DataType="pages:RicercaPage"
			 x:Class="Diario.Pages.RicercaPage">
	<StackPanel>
		<Label x:Name="RicercaL">Cerca tutti i topic a partitre da:</Label>
		<DatePicker x:Name="filtraPerData" />
		<Button x:Name="Cerca" Click="CercaDalClicked" />
	</StackPanel>
</UserControl>
