<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
			 xmlns:pages="clr-namespace:Diario.Pages"
			 x:DataType="pages:HomePage"
             x:Class="Diario.Pages.HomePage">
<StackPanel>

			<ComboBox x:Name="Dati" />
			<TextBox x:Name="sstring" Text="" TextWrapping="Wrap" AcceptsReturn="True"/>
			<Button x:Name="Leggi" Click="LeggiClicked" />
		    <Button x:Name="Modifica" Click="ModificaClicked" /> 
			<Button x:Name="Inserisci" Click="InserisciClicked" />
			<Button x:Name="Elimina" Click="EliminaClicked" />
		</StackPanel>
</UserControl>
